	<select id="listarReporteEncuesta" parameterClass="java.util.HashMap" resultMap="getReporteEncuesta" > 
		SELECT 
			a.id, 
		    a.id_ticket as idTicket, 
		    e.codigo, 
		    concat(f.V_USUARIO_NOMBRES, ' ', f.V_USUARIO_AP_PATERNO, ' ', f.V_USUARIO_AP_MATERNO) as usuario,
		    g.descripcion as ventanilla,
		    b.descripcion as encuesta,
		    c.descripcion as pregunta,
		    d.descripcion as respuesta,
		    a.hora,
		    a.fecha
		from tbl_encuesta_respuesta a
			inner join tbl_encuesta b on a.id_encuesta = b.id
			inner join tbl_encuesta_preguntas c on a.id_pregunta = c.id
			inner join tbl_encuesta_opciones d on a.id_opcion = d.id
			inner join tbl_ticket e on a.id_ticket = e.id
			inner join ctrl_seg_usuario f on a.id_usuario = f.I_USUARIO_ID
			inner join tbl_ventanilla g on a.id_ventanilla = g.id
		WHERE (#idUsuario:INTEGER# = 0 or a.id_usuario = #idUsuario:INTEGER#)
			AND (#idVentanilla:INTEGER# = 0 or a.id_ventanilla = #idVentanilla:INTEGER#)
			AND (
			    #fechaInicio:DATE# = '' OR #fechaFin:DATE# = '' OR 
			    (a.fecha BETWEEN #fechaInicio:DATE# AND #fechaFin:DATE#)
			)
			GROUP BY a.id
		UNION
		SELECT 
			a.id, 
		    a.id_ticket as idTicket, 
		    e.codigo, 
		    concat(f.V_USUARIO_NOMBRES, ' ', f.V_USUARIO_AP_PATERNO, ' ', f.V_USUARIO_AP_MATERNO) as usuario,
		    g.descripcion as ventanilla,
		    b.descripcion as encuesta,
		    c.descripcion as pregunta,
		    d.descripcion as respuesta,
		    a.hora,
		    a.fecha
		from tbl_encuesta_respuesta_historico a
			inner join tbl_encuesta b on a.id_encuesta = b.id
			inner join tbl_encuesta_preguntas c on a.id_pregunta = c.id
			inner join tbl_encuesta_opciones d on a.id_opcion = d.id
			inner join tbl_ticket_historico e on a.id_ticket = e.id
			inner join ctrl_seg_usuario f on a.id_usuario = f.I_USUARIO_ID
			inner join tbl_ventanilla g on a.id_ventanilla = g.id
		WHERE (#idUsuario:INTEGER# = 0 or a.id_usuario = #idUsuario:INTEGER#)
			AND (#idVentanilla:INTEGER# = 0 or a.id_ventanilla = #idVentanilla:INTEGER#)
			AND (
			    #fechaInicio:DATE# = '' OR #fechaFin:DATE# = '' OR 
			    (a.fecha BETWEEN #fechaInicio:DATE# AND #fechaFin:DATE#)
			)
			GROUP BY a.id
		ORDER BY idTicket ASC;
	</select>  
