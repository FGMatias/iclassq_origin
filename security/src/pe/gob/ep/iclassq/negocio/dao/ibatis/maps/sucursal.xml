  	<select id="listarSucursalByEmpresa" parameterClass="java.util.HashMap" resultMap="getResultSucursal" >   		
   		SELECT t.id,t.nombre, t.url,t.ruc, t.direccion, t.telefono, a.descripcion as descripcionAlgoritmo FROM tbl_sucursal t  
		INNER JOIN tbl_algoritmo a ON a.id = t.id_tipo_algoritmo 
		WHERE t.id_empresa = #idEmpresa:INTEGER#
		   		   
	</select>
  	
	<select id="listarSucursalByUsuario" parameterClass="java.util.HashMap" resultMap="getResultSucursal" >   		
   		SELECT t.id,t.nombre, t.url,t.ruc, t.direccion, t.telefono, a.descripcion as descripcionAlgoritmo FROM tbl_sucursal t  
		INNER JOIN tbl_algoritmo a ON a.id = t.id_tipo_algoritmo 
		WHERE t.id_empresa = #idEmpresa:INTEGER# AND t.id_usuario = #adminIdUsuario:INTEGER#

	</select>
	
