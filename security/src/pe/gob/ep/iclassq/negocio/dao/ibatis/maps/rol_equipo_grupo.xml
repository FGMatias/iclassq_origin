	<select id="getGrupoDelRolEquipo"  parameterClass="java.util.HashMap" resultMap="getQueryGrupoProporcion">
			SELECT reg.id_grupo as idGrupo, g.id_algoritmo as idAlgoritmo , 
			if(suc.id_tipo_algoritmo!=4, 1, reg.proporcion) as proporcionTeorica, 
			0 as registrosActuales, 0 as porcentajeTeorica, 0 as porcentajePractico, 
			0 as diferencia 
			FROM tbl_rol_equipo_grupo reg 
			INNER JOIN tbl_grupo g on reg.id_grupo=g.id 
			INNER JOIN tbl_sucursal suc on suc.id = g.id_sucursal 
			WHERE reg.id_rol_equipo = #idRolEquipo:INTEGER#	
	</select>  
	
	
