	<select id="getCantidadRegistrosActuales" parameterClass="java.util.HashMap" resultClass="java.lang.Integer" >
    
    	SELECT COUNT(tk.id) as Cantidad 
		FROM tbl_ticket tk	
		where date_format(tk.fecha_atencion, "%y/%m/%d")= date_format(now(), "%y/%m/%d") and tk.estado = 0 
		and  tk.id_subgrupo in 
		(select sgr.id from tbl_subgrupo sgr where sgr.id_grupo= #idGrupo:INTEGER#)
    
  </select>  
  
