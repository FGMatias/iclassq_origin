  <select id="listarGruposByTiempo" parameterClass="java.util.HashMap" resultMap="getResultGrupoXTiempo" > 
  
  SELECT g.id, g.nombre, g.prefijo FROM tbl_rol_equipo_grupo rg 
		INNER JOIN tbl_grupo g ON rg.id_grupo =g.id
        WHERE rg.id_rol_equipo= #idRolEquipo:INTEGER# 
		AND CURTIME() BETWEEN g.hora_inicio AND g.hora_fin
		ORDER BY g.id  		


  	</select>
  
