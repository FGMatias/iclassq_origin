	<select id="listarGruposBySucursal" parameterClass="java.util.HashMap" resultMap="getResultGrupo" >   		
   		SELECT t.id, t.nombre, a.descripcion as nombreAlgoritmo FROM tbl_grupo t  
		INNER JOIN tbl_algoritmo a ON a.id = t.id_algoritmo 
		WHERE t.id_sucursal = #idSucursal:INTEGER#
  	</select>
 
   <select id="listargruposxparametroTipoDeriva" parameterClass="java.util.HashMap" resultMap="getResultGrupo">   		
	SELECT g.id, g.nombre, a.descripcion as nombreAlgoritmo 
   		FROM tbl_grupo g  
		INNER JOIN tbl_algoritmo a ON a.id = g.id_algoritmo 
		WHERE g.id_sucursal = #idSucursal:INTEGER#
		and (     (#tipoParametro:INTEGER#=1)
              or  ( (#tipoParametro:INTEGER#=2) 
              		and g.id_area in (select seg.I_AREA
					                    from ctrl_seg_usuario seg   
					                   where seg.I_USUARIO_ID =#idUsuario:INTEGER#) )
			  or  ( ( #tipoParametro:INTEGER#=3) 
			  		and  ( g.id in (select reg.id_grupo
	                                 from ctrl_seg_usuario seg, tbl_rol_equipo_grupo reg   
	                                where seg.I_USUARIO_ID =#idUsuario:INTEGER# 
                                        	and   seg.I_ROL_EQUIPO=reg.id_rol_equipo) ) )
             )
  	</select>

   <select id="listarGruposxventanilla" parameterClass="java.util.HashMap" resultMap="getResultGrupo">   		
		SELECT g.id, g.nombre, a.descripcion as nombreAlgoritmo 
   		FROM tbl_grupo g  
		INNER JOIN tbl_algoritmo a ON a.id = g.id_algoritmo 
		WHERE g.id_sucursal = #idSucursal:INTEGER#
		and   g.id in (select DISTINCT reg.id_grupo
                         from ctrl_seg_usuario seg, tbl_rol_equipo_grupo reg   
                        where seg.i_ventanilla = #idVentanilla:INTEGER#
                       	and   seg.I_ROL_EQUIPO=reg.id_rol_equipo
                       ) 

  	</select> 
