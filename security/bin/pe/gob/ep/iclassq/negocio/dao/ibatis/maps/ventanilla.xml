  <!--sql adicionales -->
    <select id="listarventanillaxparametroTipoDeriva" parameterClass="java.util.HashMap" resultMap="ibatorgenerated_BaseResultMap" > 
		
		SELECT v.*, a.descripcion as nombreArea
   		FROM tbl_ventanilla v  
		INNER JOIN tbl_area a ON a.id = v.id_area 
		WHERE v.id_sucursal =  #idSucursal:INTEGER#
		and (     (#tipoParametro:INTEGER#=1)
              or  ( ( #tipoParametro:INTEGER#=2) and v.id_area in (select seg.I_AREA
			                    from ctrl_seg_usuario seg   
			                   where seg.I_USUARIO_ID =#idUsuario:INTEGER#) )
			  or  ( ( #tipoParametro:INTEGER#=3) and  ( v.id in (select seg.i_ventanilla
                                            from ctrl_seg_usuario seg, tbl_rol_equipo_grupo reg 
                                            where seg.I_ROL_EQUIPO=reg.id_rol_equipo
                                            and reg.id_rol_equipo in ( select csu1.I_ROL_EQUIPO 
                                                                         from   ctrl_seg_usuario csu1 
                                                                        where csu1.I_USUARIO_ID =#idUsuario:INTEGER#)
                                          ) 
                                ) 
                  )
             )
  	</select>   
