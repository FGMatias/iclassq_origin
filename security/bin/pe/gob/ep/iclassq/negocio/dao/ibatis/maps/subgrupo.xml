  	<select id="listarSubGruposXSucursal" parameterClass="java.util.HashMap" resultMap="getBeanSubGrupo" >   		
   		SELECT sg.id as iSubGrupo, g.id as iGrupo , sg.nombre as vNombreSubGrupo, sg.descripcion as vDescripcion,  
   		CONCAT(g.prefijo,'-', sg.prefijo) as vPrefijo, sg.id_prioridad as iPrioridad, sg.secuecia_kiosko as iSecuencia    
   		FROM tbl_subgrupo sg 
		INNER JOIN tbl_grupo g on g.id = sg.id_grupo 
		INNER JOIN tbl_sucursal s on s.id = g.id_sucursal 
		WHERE g.id = #idGrupo:INTEGER# and s.id = #idSucursal:INTEGER#
		order by sg.secuecia_kiosko ASC   		   		   
  	</select>  
  	  
