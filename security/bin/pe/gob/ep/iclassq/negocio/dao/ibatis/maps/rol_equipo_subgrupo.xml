	<select id="listarSubGrupoXRol" parameterClass="java.util.HashMap" resultMap="getlistSubGrupos" > 
		SELECT a.id as idRolEquipoSubGrupo, a.id_rol_equipo as idRolEquipo, a.id_subgrupo as idSubGrupo, c.id as idGrupo, 
			   b.nombre as nombreSubGrupo, c.nombre as nombreGrupo, a.proporcion, c.prefijo
		FROM tbl_rol_equipo_subgrupo a
		INNER JOIN tbl_subgrupo b on b.id=a.id_subgrupo
		INNER JOIN tbl_grupo c on c.id=b.id_grupo
		WHERE a.id_rol_equipo = #idRolEquipo:INTEGER#
